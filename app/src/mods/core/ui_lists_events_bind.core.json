{
	"keydown": [
		{
			"selector": ".simple_uilist_text_input",
			"function": "uilist_scroller",
			"pass_event": true,
			"pass_element": true,
			"pass_params": ""
		}
	],

	"keyup": [
		{
			"selector": ".simple_uilist_text_input",
			"function": "simple_ui_list_buildsuggest",
			"pass_event": true,
			"pass_element": true,
			"pass_params": ""
		}
	],
	
	"focusout": [
		{
			"selector": ".simple_uilist_text_input",
			"function": "uilist_showhide",
			"pass_event": false,
			"pass_element": true,
			"pass_params": "false"
		}
	],
	
	"focusin": [
		{
			"selector": ".simple_uilist_text_input",
			"function": "uilist_showhide",
			"pass_event": false,
			"pass_element": true,
			"pass_params": "true"
		}
	]
}